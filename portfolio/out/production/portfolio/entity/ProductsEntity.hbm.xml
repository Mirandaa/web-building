<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ProductsEntity" table="products" schema="portfolio">
        <id name="id">
            <column name="id" sql-type="varchar(30)" length="30"/>
        </id>
        <property name="link">
            <column name="link" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="minMoney">
            <column name="min_money" sql-type="double" precision="-1" not-null="false"/>
        </property>
        <property name="title">
            <column name="title" sql-type="text" not-null="true"/>
        </property>
        <property name="earn">
            <column name="earn" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="dateStart">
            <column name="date_start" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="dateEnd">
            <column name="date_end" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="dateEarn">
            <column name="date_earn" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="days">
            <column name="days" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="surplusValue">
            <column name="surplus_value" sql-type="double" precision="-1" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>