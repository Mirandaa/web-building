<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>getNews</servlet-name>
        <servlet-class>servlet.getNews</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getNews</servlet-name>
        <url-pattern>/getNews</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>signIn</servlet-name>
        <servlet-class>servlet.signIn</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signIn</servlet-name>
        <url-pattern>/signIn</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>signUp</servlet-name>
        <servlet-class>servlet.signUp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signUp</servlet-name>
        <url-pattern>/signUp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>signOut</servlet-name>
        <servlet-class>servlet.signOut</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signOut</servlet-name>
        <url-pattern>/signOut</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>testCheck</servlet-name>
        <servlet-class>servlet.testCheck</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>testCheck</servlet-name>
        <url-pattern>/testCheck</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>getStatus</servlet-name>
        <servlet-class>servlet.getStatus</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getStatus</servlet-name>
        <url-pattern>/getStatus</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getTestStatus</servlet-name>
        <servlet-class>servlet.getTestStatus</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getTestStatus</servlet-name>
        <url-pattern>/getTestStatus</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getGroups</servlet-name>
        <servlet-class>servlet.getGroups</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getGroups</servlet-name>
        <url-pattern>/getGroups</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>group</servlet-name>
        <servlet-class>servlet.group</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>group</servlet-name>
        <url-pattern>/group</url-pattern>
    </servlet-mapping>
</web-app>